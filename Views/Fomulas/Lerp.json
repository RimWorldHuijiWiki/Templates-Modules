        "itemSize": 20,
        "iconStyle": {
            "normal": {
                "borderColor": "#c0c7da"
            },
            "emphasis": {
                "borderColor": "#e6af2e"
            }
        },
        "right": "5%",
        "top": "4%"
    },
    "tooltip": {
        "formatter": "技能系数<br>射击（{b}级）：× {c}%",
        "extraCssText": "border-radius: 0;",
