        "axisLine": {
            "onZero": false,
            "lineStyle": { "color": "#c0c7da" }
        }
    }],
    "series": [{
        "data": [50, 70, 80, 90, 93, 95, 96, 96.5, 97, 97.5, 98, 98.5, 98.75, 99, 99.1, 99.2, 99.3, 99.4, 99.5, 99.6, 99.7],
        "type": "line",
        "name": "技能系数",
        "label": {
            "normal": {
                "show": true,
                "formatter": "{c}%"
            }
        }
    }],
    "formatterStyle": "Normal",
    "backgroundColor": "#282f44"
